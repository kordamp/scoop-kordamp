{
  "version": "1.9.0",
  "description": "Checks POM files may be uploaded to Maven Central",
  "homepage": "https://kordamp.org/pomchecker",
  "license": "Apache-2.0",
  "url": "https://github.com/kordamp/pomchecker/releases/download/v1.9.0/pomchecker-1.9.0.zip",
  "hash": "sha256:54af037a2590239c40eab0967dbf2d3785710fc62929042b695432e7fb936a6f",
  "extract_dir": "pomchecker-1.9.0",
  "env_add_path": "bin",
  "suggest": {
    "JDK": [
      "java/oraclejdk",
      "java/openjdk"
    ]
  },
  "checkver": {
    "url": "https://github.com/kordamp/pomchecker/releases/latest",
    "re": "v([\\d.]+).zip"
  },
  "autoupdate": {
    "url": "https://github.com/kordamp/pomchecker/releases/download/v$version/pomchecker-$version.zip",
    "extract_dir": "pomchecker-$version",
    "hash": {
      "url": "$url.sha256"
    }
  }
}
