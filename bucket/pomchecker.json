{
  "version": "1.14.0",
  "description": "Checks POM files may be uploaded to Maven Central",
  "homepage": "https://kordamp.org/pomchecker",
  "license": "Apache-2.0",
  "url": "https://github.com/kordamp/pomchecker/releases/download/v1.14.0/pomchecker-1.14.0.zip",
  "hash": "sha256:8583cc71ab0ea2ade85791a2926204355f4c4304b81d092344f8b4dcace3bcd1",
  "extract_dir": "pomchecker-1.14.0",
  "env_add_path": "bin",
  "suggest": {
    "JDK": [
      "java/oraclejdk",
      "java/openjdk"
    ]
  },
  "checkver": {
    "url": "https://github.com/kordamp/pomchecker/releases/latest",
    "re": "v([\\d.]+).zip"
  },
  "autoupdate": {
    "url": "https://github.com/kordamp/pomchecker/releases/download/v$version/pomchecker-$version.zip",
    "extract_dir": "pomchecker-$version",
    "hash": {
      "url": "$url.sha256"
    }
  }
}
