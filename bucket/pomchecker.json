{
  "version": "1.12.0",
  "description": "Checks POM files may be uploaded to Maven Central",
  "homepage": "https://kordamp.org/pomchecker",
  "license": "Apache-2.0",
  "url": "https://github.com/kordamp/pomchecker/releases/download/v1.12.0/pomchecker-1.12.0.zip",
  "hash": "sha256:d9c2fb161265164b402bb6bd88e87dcbd8cf7c4a76b61a95b9a431994b193922",
  "extract_dir": "pomchecker-1.12.0",
  "env_add_path": "bin",
  "suggest": {
    "JDK": [
      "java/oraclejdk",
      "java/openjdk"
    ]
  },
  "checkver": {
    "url": "https://github.com/kordamp/pomchecker/releases/latest",
    "re": "v([\\d.]+).zip"
  },
  "autoupdate": {
    "url": "https://github.com/kordamp/pomchecker/releases/download/v$version/pomchecker-$version.zip",
    "extract_dir": "pomchecker-$version",
    "hash": {
      "url": "$url.sha256"
    }
  }
}
