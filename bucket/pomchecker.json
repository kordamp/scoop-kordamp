{
  "version": "1.6.0",
  "description": "Checks POM files may be uploaded to Maven Central",
  "homepage": "https://kordamp.org/pomchecker",
  "license": "Apache-2.0",
  "url": "https://github.com/kordamp/pomchecker/releases/download/v1.6.0/pomchecker-1.6.0.zip",
  "hash": "sha256:721de939fa9a1340592e37b611731797c615846d246fac7fabb3aafeaeb8e1eb",
  "extract_dir": "pomchecker-1.6.0",
  "env_add_path": "bin",
  "suggest": {
    "JDK": [
      "java/oraclejdk",
      "java/openjdk"
    ]
  },
  "checkver": {
    "url": "https://github.com/kordamp/pomchecker/releases/latest",
    "re": "v([\\d.]+).zip"
  },
  "autoupdate": {
    "url": "https://github.com/kordamp/pomchecker/releases/download/v$version/pomchecker-$version.zip",
    "extract_dir": "pomchecker-$version",
    "hash": {
      "url": "$url.sha256"
    }
  }
}
