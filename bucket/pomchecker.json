{
    "version": "1.15.0",
    "description": "Checks POM files may be uploaded to Maven Central",
    "homepage": "https://kordamp.org/pomchecker",
    "license": "Apache-2.0",
    "url": "https://github.com/kordamp/pomchecker/releases/download/v1.15.0/pomchecker-1.15.0.zip",
    "hash": "sha256:4b19b67ffee054885222cbab74b68736d930f63edb47cddb01a305fed4da3a84",
    "extract_dir": "pomchecker-1.15.0",
    "env_add_path": "bin",
    "suggest": {
        "JDK": [
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "checkver": {
        "url": "https://github.com/kordamp/pomchecker/releases/latest",
        "re": "v([\\d.]+).zip"
    },
    "autoupdate": {
        "url": "https://github.com/kordamp/pomchecker/releases/download/v$version/pomchecker-$version.zip",
        "extract_dir": "pomchecker-$version",
        "hash": {
            "url": "$url.sha256"
        }
    }
}
